#!/bin/sh
# $1 - benchmark folder
# $2 - domain
# $3 - instances
# $4 - max-budget
# $5 - exe-folder
# $6 - from-index

ulimit -t 1800;

rm -r ../exp_results
mkdir ../exp_results

for i in $(seq $6 $3);
do

	./run_experiments-single-problem-part $1 $2 $i $4 $5 simplified_joint > ../exp_results/output_$2_$i

done
