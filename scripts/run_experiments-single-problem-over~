	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-combined AM1F---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-combined AM1F


	ulimit -t 600;
	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* combined-rel-proc AM1F---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* combined-rel-proc AM1F

	ulimit -t 600;
	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* regular hmin-heur-false---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* regular hmin-heur-false

	ulimit -t 600;
	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-proc hmin-heur-false---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-proc hmin-heur-false


	ulimit -t 600;
	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-env hmin-heur-false---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-env hmin-heur-false


	ulimit -t 600;
	echo "---$1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-mod hmin-heur-false---"
	$5/umd $1/$2/joint/p$3/p$3-domain-design-all.pddl p$3\-$4-design LAO* rel-mod hmin-heur-false
	
	
